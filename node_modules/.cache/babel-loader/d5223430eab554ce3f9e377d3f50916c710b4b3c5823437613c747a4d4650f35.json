    </div>\n                <p class=\"text-lg\">Projects</p>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport VueCountUp from \"vue-countup-v3\";\n\nexport default {\n    components: {\n        VueCountUp,\n    },\n    data() {\n        return {\n            startAnimation: false,\n        };\n    },\n    mounted() {\n        this.observeSection();\n    },\n    methods: {\n        observeSection() {\n            const observer = new IntersectionObserver(\n                (entries) => {\n                    const entry = entries[0];\n                    if (entry.isIntersecting) {\n                        this.startAnimation = true; \n                        observer.disconnect();\n                    }\n                },\n                {\n                    root: null, \n                    threshold: 0.5,\n                }\n            );\n\n            observer.observe(this.$refs.countUpSection);\n        },\n    },\n};\n</script>\n"],"mappings":"AAsEA,OAAOA,UAAS,MAAO,gBAAgB;AAEvC,eAAe;EACXC,UAAU,EAAE;IACRD;EACJ,CAAC;EACDE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,cAAc,EAAE;IACpB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,cAAc,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACLD,cAAcA,CAAA,EAAG;MACb,MAAME,QAAO,GAAI,IAAIC,oBAAoB,CACpCC,OAAO,IAAK;QACT,MAAMC,KAAI,GAAID,OAAO,CAAC,CAAC,CAAC;QACxB,IAAIC,KAAK,CAACC,cAAc,EAAE;UACtB,IAAI,CAACR,cAAa,GAAI,IAAI;UAC1BI,QAAQ,CAACK,UAAU,CAAC,CAAC;QACzB;MACJ,CAAC,EACD;QACIC,IAAI,EAAE,IAAI;QACVC,SAAS,EAAE;MACf,CACJ,CAAC;MAEDP,QAAQ,CAACQ,OAAO,CAAC,IAAI,CAACC,KAAK,CAACC,cAAc,CAAC;IAC/C;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}