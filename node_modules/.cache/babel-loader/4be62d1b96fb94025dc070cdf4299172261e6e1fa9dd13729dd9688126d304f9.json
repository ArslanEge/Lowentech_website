-[#e0e0e0] to-[#FFFFFF] p-6 sm:p-10 rounded-lg\">\n        <template #content>\n          <!-- Title Section -->\n          <h1 class=\"text-3xl sm:text-4xl lg:text-5xl font-medium text-black text-center mb-16\">\n            Contact Us\n          </h1>\n  \n          <!-- Form -->\n          <form @submit.prevent=\"handleSubmit\" class=\"max-w-3xl mx-auto\">\n            <div class=\"grid grid-cols-1 sm:grid-cols-2 gap-6\">\n              <!-- First Name -->\n              <div>\n                <label class=\"block text-base font-medium text-black mb-2\">\n                  First Name\n                </label>\n                <InputTextField\n                  v-model=\"form.firstName\"\n                  class=\"w-full p-2 text-black\"\n                  placeholder=\"First Name\"\n                />\n              </div>\n  \n              <!-- Last Name -->\n              <div>\n                <label class=\"block text-base font-medium text-black mb-2\">\n                  Last Name\n                </label>\n                <InputTextField\n                  v-model=\"form.lastName\"\n                  class=\"w-full p-2 text-black\"\n                  placeholder=\"Last Name\"\n                />\n              </div>\n  \n              <!-- Email -->\n              <div class=\"sm:col-span-2\">\n                <label class=\"block text-base font-medium text-black mb-2\">\n                  Email\n                </label>\n                <InputTextField\n                  v-model=\"form.email\"\n                  type=\"email\"\n                  class=\"w-full p-2 text-black\"\n                  placeholder=\"Email address\"\n                />\n              </div>\n  \n              <!-- Message -->\n              <div class=\"sm:col-span-2\">\n                <label class=\"block text-base font-medium text-black mb-2\">\n                  Message\n                </label>\n                <TextAreaField\n                  v-model=\"form.message\"\n                  rows=\"6\"\n                  class=\"w-full p-2 text-black\"\n                  placeholder=\"Leave us a message...\"\n                  auto-resize\n                />\n              </div>\n            </div>\n  \n            <!-- Button -->\n            <div class=\"mt-10 flex justify-center\">\n              <ActionButton\n                type=\"submit\"\n                label=\"Send Message\"\n                class=\"px-6 py-3 rounded-xl w-full bg-black text-softer-gray\"\n              />\n            </div>\n          </form>\n        </template>\n      </Card>\n    </div>\n  </template>\n  \n  <script>\n  import Card from \"primevue/card\";\n  import InputText from \"primevue/inputtext\";\n  import Textarea from \"primevue/textarea\";\n  import Button from \"primevue/button\";\n  import Toast from \"primevue/toast\";\n  \n  export default {\n    components: {\n      Card,\n      Toast,\n      InputTextField: InputText, // alias\n      TextAreaField: Textarea, // alias\n      ActionButton: Button, // alias\n    },\n    data() {\n      return {\n        form: {\n          firstName: \"\",\n          lastName: \"\",\n          email: \"\",\n          message: \"\",\n        },\n      };\n    },\n    methods: {\n      handleSubmit() {\n        // Basic validation\n        if (\n          !this.form.firstName.trim() ||\n          !this.form.lastName.trim() ||\n          !this.form.email.trim() ||\n          !this.form.message.trim()\n        ) {\n          this.$toast.add({\n            severity: \"error\",\n            summary: \"Missing Fields\",\n            detail: \"Please fill in all fields before submitting.\",\n            life: 3000,\n          });\n          return;\n        }\n  \n        // Simulate success (replace with real logic)\n        this.$toast.add({\n          severity: \"success\",\n          summary: \"Message Sent\",\n          detail: \"Your message has been submitted successfully!\",\n          life: 3000,\n        });\n  \n        // Reset form\n        this.form = { firstName: \"\", lastName: \"\", email: \"\", message: \"\" };\n      },\n    },\n  };\n  </script>\n  "],"mappings":"AAiFE,OAAOA,IAAG,MAAO,eAAe;AAChC,OAAOC,SAAQ,MAAO,oBAAoB;AAC1C,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,MAAK,MAAO,iBAAiB;AACpC,OAAOC,KAAI,MAAO,gBAAgB;AAElC,eAAe;EACbC,UAAU,EAAE;IACVL,IAAI;IACJI,KAAK;IACLE,cAAc,EAAEL,SAAS;IAAE;IAC3BM,aAAa,EAAEL,QAAQ;IAAE;IACzBM,YAAY,EAAEL,MAAM,CAAE;EACxB,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb;MACA,IACE,CAAC,IAAI,CAACN,IAAI,CAACC,SAAS,CAACM,IAAI,CAAC,KAC1B,CAAC,IAAI,CAACP,IAAI,CAACE,QAAQ,CAACK,IAAI,CAAC,KACzB,CAAC,IAAI,CAACP,IAAI,CAACG,KAAK,CAACI,IAAI,CAAC,KACtB,CAAC,IAAI,CAACP,IAAI,CAACI,OAAO,CAACG,IAAI,CAAC,GACxB;QACA,IAAI,CAACC,MAAM,CAACC,GAAG,CAAC;UACdC,QAAQ,EAAE,OAAO;UACjBC,OAAO,EAAE,gBAAgB;UACzBC,MAAM,EAAE,8CAA8C;UACtDC,IAAI,EAAE;QACR,CAAC,CAAC;QACF;MACF;;MAEA;MACA,IAAI,CAACL,MAAM,CAACC,GAAG,CAAC;QACdC,QAAQ,EAAE,SAAS;QACnBC,OAAO,EAAE,cAAc;QACvBC,MAAM,EAAE,+CAA+C;QACvDC,IAAI,EAAE;MACR,CAAC,CAAC;;MAEF;MACA,IAAI,CAACb,IAAG,GAAI;QAAEC,SAAS,EAAE,EAAE;QAAEC,QAAQ,EAAE,EAAE;QAAEC,KAAK,EAAE,EAAE;QAAEC,OAAO,EAAE;MAAG,CAAC;IACrE;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}